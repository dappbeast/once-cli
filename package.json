{
  "name": "once-cli",
  "version": "1.0.3",
  "private": true,
  "description": "Fast, simple scripting CLI wrapping babel-node and yargs.",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "yargs": "^14.0.0"
  },
  "main": "./bin/index.js",
  "bin": {
    "once": "./bin/index.js"
  },
  "standard": {
    "ignore": [
      "**/example/**"
    ]
  },
  "keywords": [
    "babel",
    "yargs",
    "scripting",
    "script",
    "cli",
    "command",
    "parser",
    "parsing",
    "async",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/earthtojake/once-cli.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0"
  }
}
